{$R *.dfm}



procedure TMainForm.dataviewExecute(Sender: TObject);
begin
    dataViewForm := TdataViewForm.Create(self);
    dataViewForm.Show;
end;

procedure TMainForm.onCreate(Sender: TObject);
begin
  Form1 := TForm1.Create(self);
  with Form1.Create(Self) do ShowModal;
